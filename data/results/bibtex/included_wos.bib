@article{yin-et-al:2021:661199,
  author = {H. Yin and X. Xu and Y. Wang and R. Xiong},
  journal = {FRONTIERS IN ROBOTICS AND AI},
  title = {Radar-to-Lidar: Heterogeneous Place Recognition via Joint Learning},
  volume = {8},
  pages = {661199},
