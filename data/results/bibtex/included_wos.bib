@article{kim-et-al:2019:2897340,
  author = {G. Kim and B. Park and A. Kim},
  journal = {IEEE ROBOTICS AND AUTOMATION LETTERS},
  title = {1-Day Learning, 1-Year Localization: Long-Term LiDAR Localization Using
Scan Context Image},
  volume = {4},
